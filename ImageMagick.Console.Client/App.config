<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings file=".\Appsecrets.config">
    <!-- You need to configure the following entries before running the sample -->
    <!-- See readme.txt for how to get the service URL from the management portal -->
    <add key="BatchAppsServiceUrl" value="https://[service].batchapps.core.windows.net/" />
    <!-- See readme.txt for how to use the management portal to set up an unattended account and obtain a key -->
    <add key="UnattendedAccountId" value="[account id]" />
    <add key="UnattendedAccountKey" value="[account key]" /> <!-- Audience for authentication tokens - do not modify -->
    <add key="BatchAppsResource" value="https://batchapps.core.windows.net/" />
  </appSettings>
</configuration>
